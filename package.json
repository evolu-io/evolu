{
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": "10.16.x"
  },
  "scripts": {
    "deps": "rimraf 'yarn.lock' 'node_modules' '**/node_modules' && yarn",
    "dev": "yarn workspace web dev",
    "prod": "yarn workspace web build && yarn workspace web start",
    "lint": "eslint '**/*.{ts,tsx}'",
    "tsc": "tsc",
    "jest": "jest",
    "build": "yarn workspace web build && yarn workspace web export",
